{
  "engines": {
    "node": ">=5.12.0"
  },
  "name": "react-inversify",
  "version": "1.3.0",
  "description": "Dependency injection for React",
  "repository": {
    "type": "git",
    "url": "https://github.com/sergey-su/react-inversify.git"
  },
  "keywords": [
    "react",
    "inversify",
    "dependency inversion",
    "ioc",
    "di",
    "javascript",
    "typescript"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "gulp jest",
    "build": "gulp build",
    "clean": "gulp clean",
    "validate": "npm ls"
  },
  "author": "sergeysu",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "@types/react": "^16.0.10",
    "@types/react-dom": "^16.0.1",
    "@types/react-redux": "^5.0.9",
    "inversify": "^4.3.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "reflect-metadata": "^0.1.10"
  },
  "devDependencies": {
    "@types/jest": "^21.1.4",
    "@types/react-test-renderer": "^16.0.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-jest": "^2.0.0",
    "gulp-typescript": "^3.2.2",
    "install": "^0.10.1",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "npm": "^5.5.1",
    "react-test-renderer": "^16.0.0",
    "run-sequence": "^1.2.2",
    "ts-jest": "^21.1.3",
    "typescript": "^2.5.3"
  }
}
